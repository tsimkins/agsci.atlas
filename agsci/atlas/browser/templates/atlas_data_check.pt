<div class="data-dump">
    <h2>Content Issues</h2>
    
    <table class="listing errors">
        <tr>
            <th>
                Severity Level
            </th>
            <th>
                Test Title
            </th>
            <th>
                Test Description
            </th>
            <th>
                Message
            </th>
        </tr>
        <tr tal:repeat="i view/data"
            tal:attributes="class i/klass">
            <td><span tal:content="i/level" /></td>
            <td tal:content="i/check/title" />
            <td tal:content="i/check/description" />
            <td tal:content="i" />     
        </tr>

    </table>
</div>