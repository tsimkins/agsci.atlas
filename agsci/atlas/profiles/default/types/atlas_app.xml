<?xml version="1.0"?>
<object name="atlas_app" meta_type="Dexterity FTI" i18n:domain="plone"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title" i18n:translate="">App</property>
 <property name="description" i18n:translate="">None</property>
 <property name="icon_expr">string:${portal_url}/document_icon.png</property>
 <property name="factory">atlas_app</property>
 <property
    name="add_view_expr">string:${folder_url}/++add++atlas_app</property>
 <property name="link_target"></property>
 <property name="immediate_view">view</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="atlas_article_page"/>
  <element value="atlas_article_slideshow"/>
  <element value="atlas_article_video"/>
  <element value="Image"/>
  <element value="File"/>
 </property>
 <property name="allow_discussion">False</property>
 <property name="default_view">view</property>
 <property name="view_methods">
  <element value="view"/>
 </property>
 <property name="default_view_fallback">False</property>
 <property name="add_permission">cmf.AddPortalContent</property>
 <property name="klass">agsci.atlas.content.tool_application.App</property>
 <property name="behaviors">
  <element value="plone.app.dexterity.behaviors.metadata.IBasic"/>
  <element value="agsci.atlas.content.behaviors.IAtlasOwnership"/>
  <element value="plone.app.content.interfaces.INameFromTitle"/>
  <element value="agsci.atlas.content.behaviors.IAtlasInternalMetadata" />
  <element value="agsci.atlas.content.behaviors.IAtlasProductCategoryMetadata" />
  <element value="agsci.atlas.content.behaviors.IAtlasProductAttributeMetadata" />
  <element value="agsci.atlas.content.behaviors.IAtlasEPASMetadata" />
  <element value="agsci.leadimage.content.behaviors.ILeadImage" />
  <element value="agsci.atlas.content.behaviors.IAtlasAudienceSkillLevel" />
  <element value="agsci.atlas.content.behaviors.IAtlasForSaleProduct" />
  <element value="plone.app.contenttypes.behaviors.richtext.IRichText"/>
  <element value="plone.app.versioningbehavior.behaviors.IVersionable" />
  <element value="agsci.atlas.content.behaviors.IAtlasProductPageNote"/>
  <element value="agsci.atlas.content.behaviors.IHomepageTopics"/>
 </property>

  <property name="schema">agsci.atlas.content.tool_application.IApp</property>

 <property name="schema_policy">dexterity</property>
 <alias from="(Default)" to="(dynamic view)"/>
 <alias from="edit" to="@@edit"/>
 <alias from="sharing" to="@@sharing"/>
 <alias from="view" to="(selected layout)"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    description="" icon_expr="" link_target="" url_expr="string:${object_url}"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    description="" icon_expr="" link_target=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
</object>
