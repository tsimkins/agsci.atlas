<?xml version="1.0"?>
<object>

    <order manager="plone.abovecontentbody" skinname="agsci.atlas">
        <viewlet name="agsci.atlas.featured_products" insert-before="*"/>
        <viewlet name="agsci.atlas.educational_drivers" insert-after="agsci.atlas.featured_products"/>
        <viewlet name="agsci.atlas.youtube_video" insert-after="agsci.leadimage"/>
    </order>

    <order manager="plone.belowcontent" skinname="agsci.atlas">
        <viewlet name="agsci.atlas.related_products" insert-before="agsci.atlas.data_check"/>
        <viewlet name="agsci.atlas.downloads" insert-before="agsci.atlas.related_products"/>
        <viewlet name="agsci.atlas.cvent_event_link" insert-before="agsci.atlas.downloads"/>
    </order>

    <order manager="plone.belowcontentbody" skinname="agsci.atlas">
        <viewlet name="agsci.atlas.google_map" insert-after="*"/>
    </order>

    <hidden manager="plone.portalfooter" skinname="agsci.atlas">
        <viewlet name="plone.footer"/>
        <viewlet name="plone.colophon"/>
        <viewlet name="plone.site_actions"/>
    </hidden>

</object>
